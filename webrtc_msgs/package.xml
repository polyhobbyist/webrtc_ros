<?xml version="1.0"?>
<package format="3">
  <name>webrtc_ros_msgs</name>
  <version>59.0.4</version>
  <description>A collection of ROS utilities for using WebRTC with ROS</description>

  <author email="mwills@wpi.edu">Mitchell Wills</author>
  <maintainer email="lou@polyhobbyist.com">Lou A.</maintainer>

  <license>BSD</license>

  <url type="website">http://wiki.ros.org/webrtc_ros</url>
  <url type="bugtracker">https://github.com/RobotWebTools/webrtc_ros/issues</url>
  <url type="repository">https://github.com/RobotWebTools/webrtc_ros</url>

  <depend>builtin_interfaces</depend>
  
  <buildtool_depend>ament_cmake</buildtool_depend>
  <build_depend>rosidl_default_generators</build_depend>

  <exec_depend>rosidl_default_runtime</exec_depend>

  <member_of_group>rosidl_interface_packages</member_of_group>
  
  <export>
